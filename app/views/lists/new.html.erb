
<div class="row justify-content-center m-5">
  <div class="col-12 col-lg-6">
     <h1 class="text-center mb-4">Agregar lista de Genero</h1>
        <%= simple_form_for @list do |f| %>
          <h4><%= f.input :name %></h4>
          <h4><%= f.input :photo, as: :file %></h4>
          <%= f.submit 'Create List', class: "btn btn-primary mt-4 w-100" %>
        <% end %>
     <%= link_to 'Back to lists', lists_path, class: "btn btn-secondary mt-4 w-100" %>
  </div>
</div>
